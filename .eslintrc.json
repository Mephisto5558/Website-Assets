{
  "root": true,
  "env": {
    "browser": true,
    "node": false
  },
  "ignorePatterns": [
    "min/*"
  ],
  "globals": {
    "Swal": "readonly"
  },
  "extends": "@mephisto5558/eslint-config",
  "rules": {
    "max-lines": "off",
    "max-nested-callbacks": [
      "warn",
      5
    ],
    "no-return-assign": "off",
    "jsdoc/no-undefined-types": [
      "error",
      {
        "definedTypes": [
          "RequestInit"
        ]
      }
    ],
    "@stylistic/max-len": "off"
  }
}